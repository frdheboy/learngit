.m_content {
    float: left;
    width: 100%;
    border-top: 2px solid #086dae;
}
.marketindex_list li p.red{
    color: #d62829;
    font-weight: bold;
}
.marketindex_list li p.green{
    color: #168816;
    font-weight: bold;
}
.b {
    font-weight: bold;
}

/*CSS3判断响应式布局*/
/*宽度小于480的样式*/
@media all and (max-width: 480px) {
    .marketindex_list_title {
        float: left;
        width: 100%;
        background-color: #e9eaec;
        height: 30px;
        line-height: 30px;
        font-size: 15px;
        word-break: keep-all;
        white-space: nowrap;
        overflow-x: visible;
    }

    .marketindex_list_title li{
        float: left;
        width: 33%;
        border-right: 1px solid #dcdee2;
    }
    .marketindex_list_title li img{
        width: 18px;
        height: 18px;
        vertical-align:text-bottom;
    }
    .marketindex_list {
        float: left;
        width: 100%;
    }

    .marketindex_list li {
        float: left;
        width: 100%;
        border-bottom: 1px solid #dcdee2;
    }
    .marketindex_list li p{
        float: left;
        width: 33%;
        line-height: 50px;
        font-size: 20px;
    }

    .marketindex_list li p span{
        float: left;
        width: 100%;
        display: block;
        height: 30px;
        line-height: 32px;
        font-size: 16px;
    }
    .marketindex_list li p span.nom{
        line-height: 50px;
        font-size: 16px;
    }

    .marketindex_list li p em{
        float: left;
        width: 100%;
        display: block;
        height: 20px;
        line-height: 15px;
        font-size: 14px;
        font-style: normal;
        color: #d77e0a;
    }
}

/*宽度在480（包括480）到640之间的样式*/
@media all and (min-width: 480px) {
    .m_content {
        border-top: 4px solid #086dae;
    }
    .marketindex_list_title {
        float: left;
        width: 100%;
        background-color: #e9eaec;
        height: 40px;
        line-height: 40px;
        font-size: 24px;
    }

    .marketindex_list_title li{
        float: left;
        width: 33%;
        border-right: 1px solid #dcdee2;
    }
    .marketindex_list_title li img{
        width: 22px;
        height: 22px;
        vertical-align:text-bottom;
    }
    .marketindex_list {
        float: left;
        width: 100%;
    }

    .marketindex_list li {
        float: left;
        width: 100%;
        border-bottom: 2px solid #dcdee2;
    }
    .marketindex_list li p{
        float: left;
        width: 33%;
        line-height: 70px;
        font-size: 28px;
    }

    .marketindex_list li p span{
        float: left;
        width: 100%;
        display: block;
        height: 45px;
        line-height: 50px;
        font-size: 24px;
    }

    .marketindex_list li p span.nom{
        line-height: 70px;
        font-size: 24px;
    }
    .marketindex_list li p em{
        float: left;
        width: 100%;
        display: block;
        height: 25px;
        line-height: 20px;
        font-size: 24px;
        font-style: normal;
        color: #d77e0a;
    }
}


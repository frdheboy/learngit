/*CSS3判断响应式布局*/
/*宽度小于480的样式*/
@media all and (max-width: 480px) {
    section.search {
        width: 100%;
        float: left;
        border-top: 2px solid #086dae;
    }

    .search > div {
        width: 100%;
        float: left;
        border-bottom: 1px solid #e3e3e3;
        height: 45px;
        line-height: 45px;
    }

    .search > div abbr {
        display: block;
        float: left;
        height: 45px;
        line-height: 45px;
        font-size: 20px;
        color: #d77e0a;
        margin: 0 10px;
    }

    .search > div dfn {
        display: block;
        float: left;
        height: 45px;
        line-height: 45px;
        font-size: 18px;
        margin: 0 10px;
        font-style: normal;
    }

    .search > div > div {
        float: right;
        width: 30px;
        height: 30px;
        border-radius: 15px;
        background-color: #086dae;
        margin-top: 8px;
        margin-right: 15px;
        position: relative;
        box-shadow: 2px 2px 2px #bfbbbb ;
    }

    .search > div > div > i {
        display: block;
        width: 2px;
        height: 2px;
        border-top: 7px solid #FFFFFF;
        border-bottom: 7px solid #FFFFFF;
        background-color: #FFFFFF;
        position: absolute;
        left: 14px;
        top: 7px;
    }

    .search > div > div > b {
        display: block;
        width: 2px;
        height: 2px;
        border-left: 7px solid #FFFFFF;
        border-right: 7px solid #FFFFFF;
        background-color: #FFFFFF;
        position: absolute;
        left: 7px;
        top: 14px;
    }

    liulanjilu.search {
        width: 100%;
        float: left;
        border-top: 2px solid #086dae;
    }

    .liulanjilu > div {
        width: 100%;
        float: left;
        border-bottom: 1px solid #e3e3e3;
        height: 45px;
        line-height: 45px;
    }

    .liulanjilu > div abbr {
        display: block;
        float: left;
        height: 45px;
        line-height: 45px;
        font-size: 20px;
        color: #d77e0a;
        margin: 0 10px;
    }

    .liulanjilu > div dfn {
        display: block;
        float: left;
        height: 45px;
        line-height: 45px;
        font-size: 18px;
        margin: 0 10px;
        font-style: normal;
    }

    .liulanjilu > div > div {
        float: right;
        width: 30px;
        height: 30px;
        border-radius: 15px;
        background-color: #086dae;
        margin-top: 8px;
        margin-right: 15px;
        position: relative;
        box-shadow: 2px 2px 2px #bfbbbb ;
    }

    .liulanjilu > div > div > i {
        display: block;
        width: 2px;
        height: 2px;
        border-top: 7px solid #FFFFFF;
        border-bottom: 7px solid #FFFFFF;
        background-color: #FFFFFF;
        position: absolute;
        left: 14px;
        top: 7px;
    }

    .liulanjilu > div > div > b {
        display: block;
        width: 2px;
        height: 2px;
        border-left: 7px solid #FFFFFF;
        border-right: 7px solid #FFFFFF;
        background-color: #FFFFFF;
        position: absolute;
        left: 7px;
        top: 14px;
    }
    .liulanjilu > div > p {
        float: right;
        margin-right: 10px;
        display: inline;
        border: 1px solid #bfbbbb;
        border-radius: 15px;
        padding: 2px 5px;
        line-height: 25px;
        margin-top: 8px;
        color: #bfbbbb;
    }
    .search > div > p {
        float: right;
        margin-right: 10px;
        display: inline;
        border: 1px solid #bfbbbb;
        border-radius: 15px;
        padding: 2px 5px;
        line-height: 25px;
        margin-top: 8px;
        color: #bfbbbb;
    }
}


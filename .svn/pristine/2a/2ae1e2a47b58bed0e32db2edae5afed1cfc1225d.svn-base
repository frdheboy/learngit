/*CSS3判断响应式布局*/
/*宽度小于480的样式*/
@media all and (max-width: 480px) {
    .superman_big {
        float: left;
        width: 100%;
        border-top: 2px solid #086dae;
    }

    .superman_big > div {
        float: left;
        width: 100%;
    }

    .superman_big > div > div {
        width: 100%;
        position: relative;
        left: 0;
        top: 0;
    }

    .superman_big > div > div > ul {
        width: 100%;
    }

    .superman_big > div > div > ul > li {
        float: left;
    }

    .superman_big > div > div > ul > li:nth-child(1) {
        width: 30px;
        line-height: 30px;
        color: #086dae;
        font-size: 18px;
        word-break: keep-all;
        white-space: nowrap;
        margin-left: 5px;
        letter-spacing: -1px;
        height: 30px;
        margin-top: 15px;
    }

    .superman_big > div > div > ul > li:nth-child(2) {
        width: 40px;
        height: 40px;
        margin: 10px;
        border-radius: 20px;
        -moz-border-radius: 20px;
        -webkit-border-radius: 20px;
        background-image: url("../images/headerimgdemo.jpg");
        background-repeat: no-repeat;
        background-size: 40px 40px;
        background-clip: padding-box;
        background-color: #FFFFFF;
        background-position: 50% 50%;
    }

    .superman_big > div > div > ul > li:nth-child(3) > h1 {
        font-size: 15px;
        margin-top: 10px;
        text-align: left;
        font-weight: normal;
        max-width: 180px;
        word-break: keep-all;
        white-space: nowrap;
        overflow-x: hidden;
    }

    .superman_big > div > div > ul > li:nth-child(3) > p {
        font-size: 12px;
        text-align: left;
        color: #939393;
        line-height: 24px;
    }

    .superman_big > div > div > ul > li:nth-child(3) > p > span {
        display: inline-block;
        text-align: left;
        word-break: keep-all;
        white-space: nowrap;
    }

    .superman_big > div > div > ul > li:nth-child(3) > ul {
        font-size: 12px;
        text-align: left;
        color: #939393;
        line-height: 22px;
        position: relative;
        left: -5px;
        display: none;
    }

    .superman_big > div > div > ul > li:nth-child(3) > ul li > span {
        display: inline-block;
        text-align: right;
        width: 65px;
        word-break: keep-all;
        white-space: nowrap;
    }

    .superman_big > div > div > ul > li:nth-child(3) > ul li > strong {
        font-weight: normal;
        color: #000000;
    }

    .superman_big > div > div > ul > li:nth-child(3) > ul li > strong.read {
        color: #d62829;
    }

    .superman_big > div > div > a.infobtn {
        position: absolute;
        right: 15px;
        top: 16px;
        display: block;
        width: 28px;
        height: 28px;
        border-radius: 14px;
        -moz-border-radius: 14px;
        -webkit-border-radius: 14px;
        background-color: #31bce9;
    }

    .superman_big > div > div > a.infobtn:before {
        content: " ";
        position: absolute;
        right: 10px;
        top: 9px;
        display: block;
        width: 9px;
        height: 9px;
        border-right: 2px solid #fff;
        border-top: 2px solid #fff;
        -webkit-transform: rotate(45deg);
        transform: rotate(45deg);
    }

    .superman_big > div.hr {
        float: left;
        width: 100%;
        height: 13px;
        background-color: #dfdfdf;
    }

    .superman_big > div > div > ul > li:nth-child(1).first {
        width: 30px;
        background: url("../images/figure_012.jpg");
        background-position: 2px 0px;
        background-size: 112px 28px;
        background-repeat: no-repeat;
        font-size: 12px;
    }

    .superman_big > div > div > ul > li:nth-child(1).second {
        width: 30px;
        background: url("../images/figure_012.jpg");
        background-position: -40px 0px;
        background-size: 112px 28px;
        background-repeat: no-repeat;
        font-size: 12px;
    }

    .superman_big > div > div > ul > li:nth-child(1).third {
        width: 30px;
        background: url("../images/figure_012.jpg");
        background-position: -82px 0px;
        background-size: 112px 28px;
        background-repeat: no-repeat;
        font-size: 12px;
    }

    .superman_big > div > div > ul > li:nth-child(1).nomarl {
        font-size: 22px;
        font-weight: bold;
    }
}


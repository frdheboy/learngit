/*CSS3判断响应式布局*/
/*宽度小于480的样式*/
@media all and (max-width: 480px) {
    .newtabs {
        float: left;
        width: 100%;
        border-top: 2px solid #086dae;
        padding-bottom: 10px;
        border-bottom: 1px solid #e5e5e5;
    }

    .newtabs ul {
        float: left;
        width: 100%;
        border-top: 1px solid #FFFFFF;
    }

    .newtabs ul li a {
        display: block;
        float: left;
        width: 100%;
    }

    .newtabs ul li a.currect {
        display: block;
        float: left;
        width: 100%;
        color: #FFFFFF;
        background-color: #086dae;
    }

    .newtabs ul li {
        float: left;
        width: 18%;
        margin-left: 1.7%;
        background-color: #FFFFFF;
        line-height: 40px;
        font-size: 14px;
        text-align: center;
        margin-top: 8px;
    }

    .newtitle {
        float: left;
        width: 100%;
    }

    .newtitle ul {
        float: left;
        width: 100%;
    }

    .newtitle ul li {
        width: 100%;
        float: left;
        border-top: 1px solid #FFFFFF;
        border-bottom: 1px solid #e5e5e5;
        height: 50px;
        position: relative;
        padding: 5px 0px;
    }

    .newtitle ul li a {
        width: 100%;
        float: left;
        height: 50px;
        display: block;
        color: #5a5a5a;
    }

    .newtitle ul li a p {
        float: left;
        font-size: 16px;
        line-height: 24px;
        width: 85%;
        text-align: left;
        padding-right: 5%;
    }

    .newtitle ul li a span {
        /*width: 10%;*/
        margin-left: 12px;
        margin-top: 8px;
        margin-right: 10px;
        float: left;
    }

    .newtitle ul li a i {
        font-style: normal;
        font-size: 12px;
        position: absolute;
        right: 20px;
        bottom: 10px;
        color: #939393;
    }

    .circle {
        display: block;
        width: 10px;
        height: 10px;
        background-color: #0b6faf;
        -webkit-border-radius: 5px;
    }
}

/*宽度在大于480（包括480）的样式*/
@media all and (min-width: 480px) {
    .newtabs {
        float: left;
        width: 100%;
        border-top: 2px solid #086dae;
        padding-bottom: 15px;
        border-bottom: 2px solid #e5e5e5;
    }

    .newtabs ul {
        float: left;
        width: 100%;
        border-top: 2px solid #FFFFFF;
    }

    .newtabs ul li a {
        display: block;
        float: left;
        width: 100%;
    }

    .newtabs ul li a.currect {
        display: block;
        float: left;
        width: 100%;
        color: #FFFFFF;
        background-color: #086dae;
    }

    .newtabs ul li {
        float: left;
        width: 18%;
        margin-left: 1.7%;
        background-color: #FFFFFF;
        line-height: 55px;
        font-size: 22px;
        text-align: center;
        margin-top: 15px;
    }

    .newtitle {
        float: left;
        width: 100%;
    }

    .newtitle ul {
        float: left;
        width: 100%;
    }

    .newtitle ul li {
        width: 100%;
        float: left;
        border-top: 2px solid #FFFFFF;
        border-bottom: 2px solid #e5e5e5;
        height: 80px;
        position: relative;
        padding: 15px 0px;
    }

    .newtitle ul li a {
        width: 100%;
        float: left;
        height: 50px;
        display: block;
        color: #5a5a5a;
    }

    .newtitle ul li a p {
        float: left;
        font-size: 26px;
        line-height: 40px;
        width: 87%;
        text-align: left;
        padding-right: 3%;
    }

    .newtitle ul li a span {
        /*width: 10%;*/
        margin-left: 12px;
        margin-top: 12px;
        margin-right: 10px;
        float: left;
    }

    .newtitle ul li a i {
        font-style: normal;
        font-size: 20px;
        position: absolute;
        right: 20px;
        bottom: 10px;
        color: #939393;
    }

    .circle {
        display: block;
        width: 16px;
        height: 16px;
        background-color: #0b6faf;
        -webkit-border-radius: 8px;
    }

}


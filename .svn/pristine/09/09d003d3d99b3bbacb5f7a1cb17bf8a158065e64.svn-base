.m_content {
    float: left;
    width: 100%;
    border-top: 2px solid #086dae;
}

/*CSS3判断响应式布局*/
/*宽度小于480的样式*/
@media all and (max-width: 480px) {
    .entrust_list_title {
        float: left;
        width: 100%;
        background-color: #e9eaec;
        height: 30px;
        font-size: 16px;
        line-height: 30px;
    }

    .entrust_list_title li {
        float: left;
        width: 20%;
    }

    .entrust_list {
        float: left;
        width: 100%;
    }

    .entrust_list li {
        float: left;
        width: 100%;
        height: 50px;
        border-bottom: 1px solid #e3e3e3;
    }

    .entrust_list li p {
        float: left;
        height: 100%;
        line-height: 50px;
        font-size: 16px;
    }

    .entrust_list li p span {
        float: left;
        height: 30px;
        width: 100%;
        line-height: 35px;
        font-size: 18px;
    }

    .entrust_list li p em {
        display: block;
        font-style: normal;
        float: left;
        height: 20px;
        width: 100%;
        line-height: 20px;
        font-size: 16px;
        color: #f5a60d;
    }

    .entrust_list_style01 {
        width: 18%;
    }

    .entrust_list_style02 {
        width: 24%;
    }

    .entrust_list_style03 {
        width: 18%;
    }

    .entrust_list_style04 {
        width: 20%;
        font-size: 22px !important;
    }

    .entrust_list_style05 {
        width: 20%;
        color: #d62829;
    }

}

/*宽度在480（包括480）到640之间的样式*/
@media all and (min-width: 480px) {
    .m_content {
        border-top: 4px solid #086dae;
    }

    .entrust_list_title {
        float: left;
        width: 100%;
        background-color: #e9eaec;
        height: 45px;
        font-size: 24px;
        line-height: 45px;
    }

    .entrust_list_title li {
        float: left;
        width: 20%;
    }

    .entrust_list {
        float: left;
        width: 100%;
    }

    .entrust_list li {
        float: left;
        width: 100%;
        height: 70px;
        border-bottom: 2px solid #e3e3e3;
    }

    .entrust_list li p {
        float: left;
        height: 100%;
        line-height: 70px;
        font-size: 24px;
    }

    .entrust_list li p span {
        float: left;
        height: 40px;
        width: 100%;
        line-height: 45px;
        font-size: 28px;
    }

    .entrust_list li p em {
        display: block;
        font-style: normal;
        float: left;
        height: 30px;
        width: 100%;
        line-height: 30px;
        font-size: 22px;
        color: #f5a60d;
    }

    .entrust_list_style01 {
        width: 18%;
    }

    .entrust_list_style02 {
        width: 24%;
    }

    .entrust_list_style03 {
        width: 18%;
    }

    .entrust_list_style04 {
        width: 20%;
        font-size: 22px !important;
    }

    .entrust_list_style05 {
        width: 20%;
        color: #d62829;
    }

}


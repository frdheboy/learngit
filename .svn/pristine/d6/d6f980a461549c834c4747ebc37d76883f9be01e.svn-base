$(document).ready(function() {
		$("#sortkeys").find("li").on("click",function(){
			var _index = $(this).index();
			var scrolltodom=$(".mcontent").find("ul:eq("+_index+")");
			var a=$("html,body").width();
			if(a<480){
				$('html,body').animate({//让点击scrolldom元素，最先显示对应的元素
				scrollTop:$(scrolltodom).offset().top-70},100
			);
			}else{
			$('html,body').animate({//让点击scrolldom元素，最先显示对应的元素
				scrollTop:$(scrolltodom).offset().top-114},100
			);
			}
		})
	
});
